{
  "expo": {
    "name": "Dream Market",
    "slug": "dream-market-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Dream_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Dream_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dreammarket.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Dream_logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dreammarket.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/Dream_logo.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#2F8F46",
          "sounds": [],
          "mode": "production",
          "androidMode": "default",
          "androidCollapsedTitle": "Nouvelles notifications"
        }
      ]
    ]
  }
}
